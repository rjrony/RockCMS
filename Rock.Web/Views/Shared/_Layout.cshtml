@model List<Rock.DataContract.MenuContract>
<!DOCTYPE html>
<html>
<head>
    <meta content="text/html; charset=gb2312" />
    <title>@ViewBag.Title</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <link href="@Url.Content("/Resources/js/ligerUI/lib/ligerUI/skins/Aqua/css/ligerui-all.css")" rel="stylesheet" type="text/css"/>
    @RenderSection("HeadCSS",false)
    @RenderSection("HeadScript", false)
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="@ViewBag.BodyClass" style="@ViewBag.BodyStyle">
    @RenderBody()
    <script src="@Url.Content("/Resources/js/ligerUI/lib/jquery/jquery-1.3.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("/Resources/js/ligerUI/lib/ligerUI/js/ligerui.min.js")" type="text/javascript"></script>
    @RenderSection("BottomScript", false)
</body>
<!-- END BODY -->
</html>
